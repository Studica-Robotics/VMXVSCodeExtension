{
	"name": "vmxpi",
	"displayName": "vmxpi",
	"description": "",
	"version": "0.0.1",
	"publisher": "Kaden",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vmxpi.updateWPILib",
		"onCommand:vmxpi.setVMXPiTarget",
		"onCommand:vmxpi.setRoboRIOTarget"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vmxpi.updateWPILib",
				"title": "Update WPILib Version",
				"category": "VMX-Pi"
			},
			{
				"command": "vmxpi.setVMXPiTarget",
				"title": "Change the deploy target to VMX-Pi (from RoboRIO)",
				"category": "VMX-Pi"
			},
			{
				"command": "vmxpi.setRoboRIOTarget",
				"title": "Change the deploy target to RoboRIO (from VMX-Pi)",
				"category": "VMX-Pi"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/node-fetch": "^2.5.7",
		"@types/vscode": "^1.41.0",
		"@types/xml2js": "^0.4.5",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"node-fetch": "^2.6.0",
		"xml2js": "^0.4.23"
	}
}
